{
  "name": "sophie-serverless-self-healing",
  "description": "Sophie AI Serverless Worker with Self-Healing Recovery (Session 749)",
  "dockerImage": "ghcr.io/kanevry/sophie-serverless-worker:latest",
  "imagePullPolicy": "Always",
  "containerDiskInGb": 50,
  "volumeInGb": 0,
  "volumeMountPath": "/workspace",
  "gpu": {
    "type": "RTX 4090",
    "count": 1
  },
  "env": [
    {
      "key": "MODEL_PATH",
      "value": "/workspace/models/buchhaltgenie-universal-v5-Q4_K_M.gguf"
    },
    {
      "key": "N_GPU_LAYERS",
      "value": "999"
    },
    {
      "key": "CTX_SIZE",
      "value": "4096"
    },
    {
      "key": "BATCH_SIZE",
      "value": "512"
    },
    {
      "key": "N_PARALLEL",
      "value": "4"
    },
    {
      "key": "HEALTH_CHECK_INTERVAL",
      "value": "30"
    },
    {
      "key": "RECOVERY_ENABLED",
      "value": "true"
    },
    {
      "key": "LLAMA_SERVER_PORT",
      "value": "8080"
    },
    {
      "key": "SENTRY_DSN",
      "value": "${SENTRY_DSN}"
    }
  ],
  "ports": "8080/http",
  "scalerType": "QUEUE_DELAY",
  "scalerValue": 4,
  "gpuTypeId": "NVIDIA RTX 4090",
  "minWorkers": 0,
  "maxWorkers": 3,
  "idleTimeout": 30,
  "flashboot": true,
  "startJupyter": false,
  "startSsh": false,
  "networkVolumeId": null,
  "templateType": "serverless"
}
